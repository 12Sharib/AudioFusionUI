{
  "expo": {
    "name": "AudioFusionUI",
    "slug": "AudioFusionUI",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
  
    "android": {
     
      "permissions": ["WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE"],
      "PermissionStatus.GRANTED": "granted"
      
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    

    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
